{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information",
      "Hangfire": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Local_Mysql": "server=localhost;database=fastframedb;user=root;password=;port=3306;Allow User Variables=True",
    "RedisConnection": "localhost,channelPrefix=f:,defaultDatabase=0"
  },
  "ResourceOption": {
    "BasePath": "../../../files",
    "FileSizeLimit": 10485760,
    "DownLoadPathRegexText": "/resources/download/([^/]+)/?((.+))?",
    "ThumbnailPathRegexText": "/resources/thumbnail/([^/]+)/?((.+))?",
    "UploadPathRegexText": "/resources/upload"
  },
  "rsaConfig": {
    "privateKeyFileName": "./privateKey.key",
    "publicKeyFileName": "./publicKey.key"
  },
  "IdentityConfig": {
    "FailCount": 3,
    "FailTime": "00:10:00",
    "TokenEffectiveTime": "1.00:00:00"
  },
  "ReverseProxy": {
    "Routes": {
      "routeAll": {
        "ClusterId": "clusterBaidu",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "clusterBaidu": {
        "Destinations": {
          "baidu": {
            "Address": "https://www.baidu.com/"
          }
        }
      }
    }
  }
}
